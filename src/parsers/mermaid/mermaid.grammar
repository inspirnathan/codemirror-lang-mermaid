// See this link for entire Pie chart syntax in Mermaid: https://mermaid.js.org/syntax/pie.html#syntax
@top MermaidDiagram {
  preDiagramLine* (PieDiagram | MindmapDiagram)
}

@skip { space }

@tokens {
  space { $[ \t\r]+ }
}

@external tokens diagramText from "./tokens" { preDiagramLine, PieDiagram, MindmapDiagram }

@detectDelim